{
  "manifest_version": 3,
  "name": "AI File Manager",
  "version": "1.0.0",
  "description": "AI-powered file management assistant",
  "permissions": [
    "storage",
    "activeTab",
    "fileSystem"
  ],
  "host_permissions": [
    "https://ai-powered-iwnx.onrender.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
